<p id="notice"><%= notice %></p>

<h1>Breweries</h1>


<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>State</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @breweries.each do |brewery| %>
      <tr>
      <td><%= brewery.name %></td>
      <td><%= brewery.address %></td>
      <td><%= brewery.city %></td>
      <td><%= brewery.state %></td>
      <td><%= brewery.state %></td>
      <td><%= brewery.country %></td>
      <td><%= link_to 'Show', brewery %></td>
      <td><%= link_to 'Edit', edit_brewery_path(brewery) %></td>
      <td><%= link_to 'Destroy', brewery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <% end %>
  </tbody>
</table>
<%= paginate @breweries %>

<%= page_entries_info @breweries %>

